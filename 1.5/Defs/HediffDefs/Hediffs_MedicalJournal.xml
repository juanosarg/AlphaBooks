<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<HediffDef>
		<defName>ABooks_MedicalJournal</defName>
		<label>medical expertise</label>
		<description>Reading a medical journal has increased the doctoring abilities of this person.</description>
		<hediffClass>HediffWithComps</hediffClass>
		<defaultLabelColor>(0.52, 1, 0.95)</defaultLabelColor>
		<isBad>false</isBad>
		<allowMothballIfLowPriorityWorldPawn>true</allowMothballIfLowPriorityWorldPawn>
		<comps>
			<li Class="AlphaBooks.HediffCompProperties_DeleteAfterTime">
				<disappearsAfterTicks>180000</disappearsAfterTicks>
			</li>
			<li Class="HediffCompProperties_DisappearsOnDeath"/>
		</comps>
		<stages>
			<li>
				<label>awful</label>
				<minSeverity>0</minSeverity>
				<statFactors>
					<MedicalTendSpeed>1.25</MedicalTendSpeed>
					<MedicalTendQuality>0.75</MedicalTendQuality>
					<MedicalOperationSpeed>1.25</MedicalOperationSpeed>
					<MedicalSurgerySuccessChance>0.75</MedicalSurgerySuccessChance>
				</statFactors>
				
				
			</li>
			<li>
				<label>poor</label>
				<minSeverity>0.1</minSeverity>
				<statFactors>
					<MedicalTendSpeed>1.1</MedicalTendSpeed>
					<MedicalTendQuality>0.85</MedicalTendQuality>
					<MedicalOperationSpeed>1.1</MedicalOperationSpeed>
					<MedicalSurgerySuccessChance>0.9</MedicalSurgerySuccessChance>
				</statFactors>
			</li>
			<li>
				<label>normal</label>
				<minSeverity>0.2</minSeverity>
				<statFactors>
					<MedicalTendSpeed>1.05</MedicalTendSpeed>
					<MedicalOperationSpeed>1.05</MedicalOperationSpeed>
					<MedicalTendQuality>1.05</MedicalTendQuality>
				</statFactors>
			</li>
			<li>
				<label>good</label>
				<minSeverity>0.3</minSeverity>
				<statFactors>
					<MedicalTendSpeed>1.1</MedicalTendSpeed>
					<MedicalTendQuality>1.05</MedicalTendQuality>
					<MedicalOperationSpeed>1.1</MedicalOperationSpeed>
					<MedicalSurgerySuccessChance>1.05</MedicalSurgerySuccessChance>
				</statFactors>
			</li>
			<li>
				<label>excellent</label>
				<minSeverity>0.4</minSeverity>
				<statFactors>
					<MedicalTendSpeed>1.2</MedicalTendSpeed>
					<MedicalTendQuality>1.15</MedicalTendQuality>
					<MedicalOperationSpeed>1.2</MedicalOperationSpeed>
					<MedicalSurgerySuccessChance>1.15</MedicalSurgerySuccessChance>
				</statFactors>
			</li>
			<li>
				<label>masterwork</label>
				<minSeverity>0.5</minSeverity>
				<statFactors>
					<MedicalTendSpeed>1.35</MedicalTendSpeed>
					<MedicalTendQuality>1.2</MedicalTendQuality>
					<MedicalOperationSpeed>1.35</MedicalOperationSpeed>
					<MedicalSurgerySuccessChance>1.2</MedicalSurgerySuccessChance>
				</statFactors>
			</li>
			<li>
				<label>legendary</label>
				<minSeverity>0.6</minSeverity>
				<statFactors>
					<MedicalTendSpeed>1.5</MedicalTendSpeed>
					<MedicalTendQuality>1.25</MedicalTendQuality>
					<MedicalOperationSpeed>1.5</MedicalOperationSpeed>
					<MedicalSurgerySuccessChance>1.25</MedicalSurgerySuccessChance>
				</statFactors>
			</li>
		</stages>
	</HediffDef>
	
</Defs>