<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RulePackDef>
		<defName>ABooks_MedicalJournal_Namer</defName>
		<include>
			<li>ArtNameUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<!-- Roots -->
				<li>title(p=3)->[titleNaked]</li>
				<li>title->[ANYPAWN_nameIndef]'s [titleNaked]</li>
				<li>title->[Character]'s [medicalStory]</li>
				<li>title->[Character]'s [objectMedical]</li>
				<li>title->[Character] [linkagePlace] [place]</li>
			
				<li>title->The [adjective] [objectMedical]</li>
				<li>titleNaked->[ConceptMedical]</li>
				<li>titleNaked->[adjective] [ConceptMedical]</li>
				<li>titleNaked->[ConceptMedical] [linkage] [ConceptMedical]</li>
				<li>titleNaked->[ConceptMedical] [linkagePlace] [place]</li>
				<li>titleNaked->[objectMedical] [linkage] [ConceptMedical]</li>
				<!-- Content -->
				<li>objectMedical->Surgery</li>
				<li>objectMedical->Prosthetics</li>
				<li>objectMedical->Medicine</li>
				<li>objectMedical->Healing</li>
				<li>objectMedical->Anatomy</li>
				<li>objectMedical->Diagnostics</li>
				<li>objectMedical->Therapy</li>
				<li>objectMedical->Vaccines</li>
				<li>objectMedical->Pathology</li>
				<li>objectMedical->Pharmacology</li>
				<li>ConceptMedical->Health</li>
				<li>ConceptMedical->Infection</li>
				<li>ConceptMedical->Recovery</li>
				<li>ConceptMedical->Disease</li>
				<li>ConceptMedical->Trauma</li>
				<li>ConceptMedical->Well-being</li>
				<li>ConceptMedical->Mortality</li>
				<li>ConceptMedical->Immunity</li>
				<li>ConceptMedical->Care</li>
				<li>ConceptMedical->Vitality</li>
				<li>linkage->of</li>
				<li>linkage->by</li>
				<li>linkage->for</li>
				<li>linkage->without</li>
				<li>linkage->with</li>
				<li>linkagePlace->in the</li>
				<li>linkagePlace->by the</li>
				<li>linkagePlace->on the</li>
				<li>linkagePlace->Near the</li>
				<li>adjective->Essential</li>
				<li>adjective->Advanced</li>
				<li>adjective->Comprehensive</li>
				<li>adjective->Pioneering</li>
				<li>adjective->Critical</li>
				<li>adjective->Life-saving</li>
				<li>adjective->Experimental</li>
				<li>adjective->Practical</li>
				<li>adjective->Intriguing</li>
				<li>adjective->Unprecedented</li>
				<li>place(p=25)->[TerrainFeature]</li>
				<li>place->Hospital</li>
				<li>place->Clinic</li>
				<li>place->Operating Room</li>
				<li>place->Recovery Ward</li>
				<li>place->Laboratory</li>
				<li>place->Research Center</li>
				<li>place->Field Station</li>
				<li>place->Emergency Room</li>
				<li>place->Medical Outpost</li>
				<li>place->Sanctuary</li>
				<li>place->Sanitarium</li>
